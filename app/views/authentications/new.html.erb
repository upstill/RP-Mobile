<% if !@area || (@area == "page") %>
<% title "Sign In Options" %>
<%= render partial: 'shared/authentications_sidebar' %>
<% end %>
<%= dialogHeader :signIn, "Time to Sign In" %>
  <% if @area && (@area == "at_top") %>
    <%= render partial: 'shared/authentications_signin_at_top' %>
  <% else %>
    <%= render partial: 'shared/authentications_signup_or_signin' %>
  <% end %>
<%= dialogFooter %>
